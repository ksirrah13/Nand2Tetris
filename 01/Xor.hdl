/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=nota);
    Not(in=b,out=notb);
    Or(a=a,b=b,out=c1);
    Or(a=nota,b=notb,out=c2);
    And(a=c1,b=c2,out=out);
}